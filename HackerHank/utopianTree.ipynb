    "def utopianTree(n):\n",
    "\n",
    "    height = 0\n",
    "    state = True\n",
    "    \n",
    "    for _ in range(n+1):\n",
    "\n",
    "        if state:\n",
    "            height+=1\n",
    "            state = not state\n",
    "        else:\n",
    "            height*=2\n",
    "            state = not state\n",
    "    \n",
    "    return height\n",
    "\n",
    "utopianTree(4)"
   ]
  }
 ],
