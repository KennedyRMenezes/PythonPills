    "def angryProfessor(k, a):\n",
    "    \n",
    "    numOfNegatives = len(list(filter(lambda x:  x <= 0, a)))\n",
    "    \n",
    "    if numOfNegatives >= k:\n",
    "        return \"NO\"\n",
    "    else:\n",
    "        return \"YES\"\n",
    "\n",
    "\n",
    "limit = 2\n",
    "a = [-1, 0, 1, 2]\n",
    "\n",
    "print(angryProfessor(limit, a))\n"
   ]
